{
  "name": "test_tailwindcss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "tsc-electron": "tsc -b tsconfig.electron.json",
    "electron": "npm run tsc-electron && electron .",
    "start": "npm run dev",
    "build": "npm run tsc-electron && vite build",
    "package": "npm run build && npx electron-builder",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.yourdomain.myapp",
    "productName": "My Electron App",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@react-three/drei": "10.5.2",
    "@react-three/fiber": "9.2.0",
    "@tailwindcss/vite": "4.1.11",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "framer-motion": "12.23.6",
    "lucide-react": "0.525.0",
    "motion": "12.23.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "6.3.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "three": "0.178.0",
    "use-sound": "5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "0.178.1",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "9.2.0",
    "electron": "37.2.6",
    "electron-builder": "26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tw-animate-css": "1.3.5",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "wait-on": "8.0.4"
  }
}