{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "outDir": "dist-electron",
        "esModuleInterop": true,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "sourceMap": true,
        "types": [
            "node"
        ],
        "baseUrl": "./"
    },
    "include": [
        "src-electron/**/*"
    ],
    "exclude": [
        "dist-electron"
    ]
}